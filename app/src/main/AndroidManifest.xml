<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myvaccine">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_vaccineimg"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_vaccineimg"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyVaccine">
        <activity android:name=".SlotActivity"></activity>
        <activity android:name=".CenterActivity" />
        <activity
            android:name=".MainActivity"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>